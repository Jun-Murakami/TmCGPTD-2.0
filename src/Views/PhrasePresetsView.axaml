            Focusable="False"
            HotKey="Ctrl+NumPad5" />
        <TextBox
            Name="TextBox5"
            Grid.Row="0"
            Grid.Column="9"
            Margin="2,0,3,0"
            Classes.clearButton="True"
            Text="{Binding Phrases[4], Mode=TwoWay}" />

        <Button
            Name="Button6"
            Grid.Row="1"
            Grid.Column="0"
            Click="OnButtonClick"
            Content="6"
            Focusable="False"
            HotKey="Ctrl+NumPad6" />
