<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d"
             d:DesignWidth="800"
             d:DesignHeight="450"
             xmlns:views="clr-namespace:TmCGPTD.Views"
             x:Class="TmCGPTD.Views.Editor2TextBoxView">
    <Panel>
        <TextBox VerticalAlignment="Stretch"
                 AcceptsReturn="True"
                 Text="{Binding Editor5_2Text}"
                 FontSize="{Binding Path=DataContext.EditorCommonFontSize, RelativeSource={RelativeSource FindAncestor, AncestorType=views:EditorView}}"
                 Name="Editor2TextBox"
                 InputMethod.IsInputMethodEnabled="True" />
    </Panel>
</UserControl>